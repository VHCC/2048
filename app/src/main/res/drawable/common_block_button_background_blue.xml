<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="#FF9E9E9E"/>
            <corners android:radius="5dp"/>
            <stroke android:width="1dp" android:color="#FF959595"/>
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <solid android:color="#FFBCBCBC"/>
            <corners android:radius="5dp"/>
            <stroke android:width="1dp" android:color="#FFBCBCBC"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#FF187DD1"/>
            <corners android:radius="5dp"/>
            <stroke android:width="1dp" android:color="#FF187DD1"/>
        </shape>
    </item>
</selector>